{
    "name": "szyku/php-nltk-client",
    "description": "PHP client implementation for szyku/nltk-api",
    "type": "library",
    "require": {
        "php" : "^5.6 || ^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "marc-mabe/php-enum": "^2.3",
        "beberlei/assert": "^2.7",
        "symfony/serializer": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "config": {
      "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Szyku\\NLTK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Szyku\\NLTK\\": "tests/"
        }
    },
    "license": "MIT",
    "homepage": "https://github.com/szyku/nltk-api",
    "authors": [
