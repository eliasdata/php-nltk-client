    "type": "library",
    "require": {
        "php" : "^5.5 || ^7.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "config": {
      "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
